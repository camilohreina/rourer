{
  "name": "rourer",
  "description": "Un router minimalistico para React.",
  "author": "Camilo Hernandez <camilohreina.dev@gmail.com>",
  "private": false,
  "keywords": [
    "react",
    "router",
    "routing",
    "enrutador",
    "react-router",
    "minify-router"
  ],
  "version": "0.0.3",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "repository": "camilohreina/rourer",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "swc ./src/*.jsx -d lib && rm lib/App.js lib/main.js lib/Router.test.js && cp src/*.js lib",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "path-to-regexp": "6.2.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.74",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "happy-dom": "^10.8.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.1"
  }
}
